<% semantic_form_for @artist do |f| %>
  <%= f.input :name %>
  <%= f.input :nationality, :as => :country %>
  <%= f.input :gender, :as => :radio, :collection => [["Male", false], ["Female", true]] %>
  <%= f.input :birth, :as => :date %>
  <%= f.input :videos, :collection => @videos_ids, :as => :check_boxes %>
  <%= f.commit_button %>
<% end %>
