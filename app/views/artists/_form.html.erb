<% semantic_form_for @artist do |f| %>
  <%= f.input :name %>
  <%= f.input :country, :as => :country %>
  <%= f.input :sex, :as => :radio, :collection => ["Male", "Female"] %>
  <%= f.input :birth, :as => :date, :start_year => 1900 %>
  <%= f.input :videos, :as => :check_boxes, :collection => @video_ids %>
  <%= f.commit_button %>
<% end %>
