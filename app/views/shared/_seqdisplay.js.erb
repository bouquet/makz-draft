var _boxes = $(".box").hide();
var _counts = $(".count").hide();
_boxes.each(function(i) {
  $(this).delay(160*i).fadeIn(800);
});
$(".box h1").hide();
$(".box").hover(function() {
  $(this).find("h1").fadeIn("fast");
}, function() {
  $(this).find("h1").fadeOut("fast");
});
_counts.delay(_boxes.length*120).fadeIn(1600);
